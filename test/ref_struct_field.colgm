use std::panic::{ assert };
use std::io::{ io };

struct A {
    a: i64&
}

func main() -> i64 {
    var a = 1;
    var b = A { a: a };

    var c: i64& = a;

    b.a -= 1;

    assert(b.a == 0, "b.a != 0");
    assert(c == 0, "c != 0");
    io::stdout().out("[ref_struct_field] ok\n");
    return b.a;
}