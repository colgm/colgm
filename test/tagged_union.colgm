struct mir_block {}
struct mir_num {}
struct mir_str {}

pub union(enum) mir {
    m_block: mir_block,
    m_num: mir_num,
    m_str: mir_str
}

pub enum mir_tag {
    m_block = 0x0010,
    m_num = 0x0100,
    m_str = 0x1000
}

pub union(mir_tag) mir_with_tag {
    m_block: mir_block,
    m_num: mir_num,
    m_str: mir_str
}