struct mir_block {}
struct mir_num {}
struct mir_str {}

pub union(enum) mir {
    m_block: mir_block,
    m_num: mir_num,
    m_str: mir_str
}

pub enum mir_tag {
    m_block,
    m_num,
    m_str
}

pub union(mir_tag) mir_with_tag {
    m_block: mir_block,
    m_num: mir_num,
    m_str: mir_str
}