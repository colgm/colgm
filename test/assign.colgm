use std::io::{ io };

pub struct A {
    a: i64,
    b: i8*,
    c: i8*
}

pub func main() -> i32 {
    var a = A::__alloc__();
    var b = nil;
    a->a = 0;
    a->a += 1;
    a->a -= 2;
    a->a *= 3;
    a->a /= 4;
    a->a %= 5;
    a->b = "[assign.colgm] hello world!";
    io::stdout().out(a->b).endln();
    a->c = "[assign.colgm] another line test.";
    io::stdout().out(a->c).endln();
    if (b == nil)
        io::stdout().out("[assign.colgm] b is null pointer.").endln();
    return 0;
}