enum Test {
    aaa,
    bbb,
    ccc
}

enum Specified {
    a = 0x1,
    b = 0o17,
    c = 114514
}

func test() -> bool {
    var a = Specified::a;
    var b = Specified::b;
    var c = Specified::c;
    return (Specified::a => u64) == 0x1 &&
           (Specified::b => u64) == 0o17 &&
           (Specified::c => i64) == 114514;
}

func main() -> i32 {
    if (!test()) {
        return (-1) => i32;
    }
    var a = Test::aaa;
    if (a == Test::aaa) {
        return 0;
    } elsif (a == Test::bbb) {
        return (-1) => i32;
    }
    return (-2) => i32;
}