use std::libc::{ free };
use std::list::{ list, primitive_list };
use std::map::{ hashmap };
use std::str::{ str };

func test_hashmap() {
    var map = hashmap<i64>::new();
    map->delete();
    free(map => i8*);
}

func test_list() {
    var str_list = list<str>::new();
    var int_list = primitive_list<i64>::new();
    str_list->delete();
    int_list->delete();
    free(str_list => i8*);
    free(int_list => i8*);
}

pub func TEST_ALL() {
    test_hashmap();
    test_list();
}