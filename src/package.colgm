use std::str::{ str };
use err::report::{ report };
use std::io::{ io };
use std::hashmap::{ hashmap };

struct str_hashmap {}

pub struct package {
    err: report*
}

impl package {
    pub func new(e: report*) -> package* {
        var res = package::__alloc__();
        res->err = e;
        return res;
    }

    pub func delete(self) {
        io::stderr().out("[").green().out("package").reset().out("]")
                    .out(" delete package\n");
    }
}