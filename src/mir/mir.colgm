        var res = mir_if::__alloc__();
        res->base = mir::instance(mir_kind::mir_if, l);
        res->condition = cond;
        res->content = ctt;
        return res;
    }

    pub func delete(self) {
        if (self->condition != nil) {
            var n= self->condition => mir*;
            n->delete();
            free(n => i8*);
        }
        if (self->content != nil) {
            var n= self->content => mir*;
            n->delete();
            free(n => i8*);
        }
    }

    pub func dump(self, out: io, pkg: package*, tm: tree_maker*) {
        tm->dump_indent(out);
        if (self->condition != nil) {
            out.purple().out("MIRIf").reset();
            out.light_orange().out(" 0x").out_hex(self => i64).reset().out(" ");
            self->base.location.dump(out);

            tm->push_indent();
            if (self->content == nil) {
                tm->set_last();
            }
            self->condition->dump(out, pkg, tm);
        } else {
            out.purple().out("MIRElse").reset();
            out.light_orange().out(" 0x").out_hex(self => i64).reset().out(" ");
            self->base.location.dump(out);

            tm->push_indent();
        }

        if (self->content != nil) {
            tm->set_last();
            self->content->dump(out, pkg, tm);
        }

        tm->pop_indent();
    }
