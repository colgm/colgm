func dump_resolved_type(out: io, pkg: package*, ty: type*) {
    if (pkg == nil || ty == nil || ty->name.empty()) {
        return;
    }

    if (ty->is_array) {
        var n = ty->array_type_full_name(pkg);
        out.blue().out("[real: ").out(n.c_str).out("] ").reset();
        n.delete();
    }

    var name = ty->full_path_name(pkg, true);
    out.cyan().out("[type: ").out(name.c_str).out("] ").reset();
    name.delete();
}

