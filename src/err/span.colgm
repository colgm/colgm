use std::libc::{malloc, print};

struct span {
    begin_line: u32,
    begin_column: u32,
    end_line: u32,
    end_column: u32
}

impl span {
    func new() -> span* {
        return span::__alloc__();
    }
    func dump(self: span*) {
        self->dump();
        return;
    }
}