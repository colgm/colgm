use std::libc::{malloc, print};

struct span {
    begin_line: i64,
    begin_column: i64,
    end_line: i64,
    end_column: i64
}

impl span {
    func new() -> span* {
        return span::__alloc__();
    }
    func dump(self: span*) {
        self->dump();
        return;
    }
}