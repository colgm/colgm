use std::str::{ str };
use err::span::{ span };

pub struct colgm_struct {
    name: str,
    location: span
}

impl colgm_struct {
    pub func delete(self) {
        self->name.delete();
        self->location.delete();
    }

    pub func copy(self) -> colgm_struct* {
        var res = colgm_struct::__alloc__();
        res->name = self->name.copy_instance();
        res->location = self->location.copy_instance();
        return res;
    }
}