func puts(v: i8*) -> i32;

func main() -> i64 {
    var i = 0;
    while (i < 10) {
        if (i < 5) {
            puts("continue: i < 5");
            i += 1;
            continue;
        }
        if (i == 5) {
            i += 1;
            continue;
            puts("continue: i == 5");
        }
        puts("continue: i > 5");
        i += 1;
        continue;
    }
    i = 0;
    while (i < 10) {
        if (i < 5) {
            puts("break: i < 5");
            i += 1;
            continue;
        }
        if (i == 5) {
            i += 1;
            break;
            puts("break: i == 5");
        }
        puts("break: i > 5");
        i += 1;
        break;
    }
    return 0;
}